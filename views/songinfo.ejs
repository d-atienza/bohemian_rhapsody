<h2>Queen Songs</h2>

<table>
  <tr>
    <th>Song ID</th>
    <th>Title</th>
    <th>Album</th>
    <th>Release Year</th>
    <th>Duration (seconds)</th>
  </tr>
  <% if (songinfo.length > 0) { %>
  <% songinfo.forEach((song) => { %>
  <tr>
    <td><%= song.song_id %></td>
    <td><%= song.title %></td>
    <td><%= song.album %></td>
    <td><%= song.release_year %></td>
    <td><%= song.duration_seconds %></td>
  </tr>
  <%})%>
  <% } %>
</table>